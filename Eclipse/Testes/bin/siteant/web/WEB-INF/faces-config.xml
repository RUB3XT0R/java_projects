<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <navigation-rule>
        <from-view-id>/clientes.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>novo</from-outcome>
            <to-view-id>/newCliente.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/editCliente.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>del</from-outcome>
            <to-view-id>/delCliente.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/cursos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>novo</from-outcome>
            <to-view-id>/newCurso.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>del</from-outcome>
            <to-view-id>/delCurso.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/editCurso.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/instrutores.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/editInstrutor.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>novo</from-outcome>
            <to-view-id>/newInstrutor.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>del</from-outcome>
            <to-view-id>/delInstrutor.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/interessados.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>novo</from-outcome>
            <to-view-id>/newInteressado.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>del</from-outcome>
            <to-view-id>/delInteressado.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/editInteressado.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/editInstrutor.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/addCursoInst.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/newInstrutor.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/addCursoInst.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/newInteressado.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/addCursoInte.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/editInteressado.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>up</from-outcome>
            <to-view-id>/upCursoInte.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/maquinas.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>del</from-outcome>
            <to-view-id>/delMaquina.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/turmas.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>novo</from-outcome>
            <to-view-id>/newTurma.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>del</from-outcome>
            <to-view-id>/delTurma.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
