algoritmo "semnome"
// Função :
// Autor :
// Data : 01/04/2012
// Seção de Declarações 
var
x: inteiro
inicio
x <- 0
repita

      escreva("Entre com um número positivo ou -1 para sair:")
      leia(x)
      se x = -1 entao
         interrompa
      fimse
      escreval("Quadrado:",x^2)
      
ate falso

fimalgoritmo
