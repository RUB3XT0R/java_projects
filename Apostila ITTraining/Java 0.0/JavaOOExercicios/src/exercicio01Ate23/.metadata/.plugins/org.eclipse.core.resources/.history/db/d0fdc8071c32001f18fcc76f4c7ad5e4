
public class Teste {

	public static void main(String[] args) {
		Aluno3 al = new Aluno3("Lucas",5495,18);
		System.out.printf("%s\n%s",
				"Resultado Aluno:",al.toString());
		System.out.println();
		Professor p1 = new Professor("Júlio",32,"Cálculo",3250);
		System.out.printf("%s\n%s",
				"Resultado Professor:",p1.toString());
	}

}
